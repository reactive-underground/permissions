{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2017",
    "lib": [
      "ES2019"
    ],
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./src",
    "incremental": true,
    "paths": {
      "~/*": [
        "./*"
      ]
    }
  },
  "exclude": ["node_modules", "dist", "index.ts", "index.d.ts"]
}
